<div class="container p-5">
{{#each posts as |post|}}
      <div class='display-flex justify-space-between p-3 mb-3 user-post' data-id="{{post.id}}"> 
        <p data-id="{{post.id}}">{{post.title}}</p>
        <p data-id="{{post.id}}">{{format_date post.createdAt}}</p>
      </div>
{{/each}}
</div>
<div class="text-center">
<button class="btn btn-primary" id="postCreateBtn">Create New Post</button>
</div>
<script src="./js/dashboard.js"></script>